{
  "name": "FondForex",
  "version": "1.0.0",
  "description": "Moteur de Money Management modulaire - Système avancé de gestion de risque et de position sizing",
  "private": true,
  "scripts": {
    "dev": "npm run kill:ports && concurrently -n FRONT,BACK -c auto \"npm run dev:web\" \"npm run dev:api\"",
    "dev:api": "uvicorn backend.app.main:app --host 127.0.0.1 --port 8001 --reload",
    "dev:web": "next dev -p 3000",
    "kill:ports": "bash scripts/kill_ports.sh",
    "build": "next build",
    "start": "next start -p 3000",
    "typecheck": "tsc --noEmit || true",
    "check:front": "bash scripts/check_front.sh",
    "test": "pytest -q",
    "lint": "next lint",
    "test:once": "vitest run"
  },
  "dependencies": {
    "lightweight-charts": "^4.2.3",
    "next": "^14.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.19.11",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "depcheck": "^1.4.7",
    "eslint": "^8",
    "eslint-config-next": "14.0.0",
    "kill-port": "^2.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "typescript": "^5.9.2",
    "vitest": "^1.6.1"
  }
}
