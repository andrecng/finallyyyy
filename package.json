{
  "name": "FondForex",
  "version": "1.0.0",
  "description": "Moteur de Money Management modulaire - Système avancé de gestion de risque et de position sizing",
  "private": true,
  "scripts": {
    "dev": "npm run kill:ports && concurrently -n FRONT,BACK -c auto \"npm run dev:web\" \"npm run dev:api\"",
    "dev:api": "uvicorn backend.main:app --host 127.0.0.1 --port 8001 --reload",
    "dev:web": "next dev -p 3000",
    "kill:ports": "bash scripts/kill_ports.sh",
    "build": "next build",
    "start": "next start -p 3000",
    "typecheck": "tsc --noEmit || true",
    "check:front": "bash scripts/check_front.sh",
    "test": "pytest -q",
    "lint": "next lint",
    "test:once": "vitest run"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "lightweight-charts": "^4.2.3",
    "next": "^14.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.19.11",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "depcheck": "^1.4.7",
    "eslint": "^8",
    "eslint-config-next": "14.0.0",
    "kill-port": "^2.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "typescript": "^5.9.2",
    "vitest": "^1.6.1"
  }
}
